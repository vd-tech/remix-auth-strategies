	"engines": {
		"node": ">=20.0.0"
	},
	"files": [
		"build",
		"package.json",
		"README.md"
	],
	"exports": {
		"./azure": "./build/azure.js",
		"./auth0": "./build/auth0.js",
		"./package.json": "./package.json"
	},
	"dependencies": {
		"remix-auth": "^4.0.0",
		"remix-auth-oauth2": "^3.1.0"
	},
	"peerDependencies": {},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.17.0",
		"@biomejs/biome": "^1.8.3",
		"@total-typescript/tsconfig": "^1.0.4",
		"@types/bun": "^1.1.6",
		"consola": "^3.2.3",
		"msw": "^2.6.7",
		"typedoc": "^0.27.3",
		"typedoc-plugin-mdn-links": "^4.0.3",
		"typescript": "^5.5.4"
	}
